<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="tweets-task-flow-definition">
    <default-activity>showTweets</default-activity>
    <data-control-scope>
      <isolated/>
    </data-control-scope>
    <task-flow-reentry>
      <reentry-allowed/>
    </task-flow-reentry>
    <input-parameter-definition id="__1">
      <name>ScreenName</name>
      <value>#{pageFlowScope.ScreenName}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <view id="showTweets">
      <page>/showTweets.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
